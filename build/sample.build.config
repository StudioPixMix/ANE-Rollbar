name = Rollbar

# The location of the Flex SDK 
flex.sdk = path/to/flex/SDK

# The SWF version to compile against
swf.version = 27

# Executable files extensions (for Mac leave empty, for Windows, set to .exe and .bat)
bin.ext = .exe
bat.ext = .bat

# The javac executable folder
javac.dir = path/to/javac/directory

# Location of the Android SDK (folder containing android.jar)
android.sdk = path/to/android/sdk
ios.sdk = path/to/ios/sdk

# Using the "export" target will copy the ANE file to this location
export.dir = path/to/export/location